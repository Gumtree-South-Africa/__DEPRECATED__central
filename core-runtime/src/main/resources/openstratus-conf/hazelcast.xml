<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config-3.1.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>replyts</name>
        <password>hbilfytpiywrp98qyeuhqefjk</password>
    </group>

    <properties>
        <property name="hazelcast.jmx">true</property>
        <property name="hazelcast.version.check.enabled">false</property>
    </properties>

    <!--  local development: absolutely no need to join a cluster (this will just make the startup slower due to lookup timeouts) -->
    <network>
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="false"/>
            <aws enabled="false"/>
        </join>

    </network>

</hazelcast>
