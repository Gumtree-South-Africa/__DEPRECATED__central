<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean id="esconfig-ipaddr" class="de.mobile.ebay.service.impl.ServiceConfigBean">
        <property name="iafToken" value="${replyts2-ebayservicesfilters-plugin.ip.iaftoken}" />
        <property name="appName" value="${replyts2-ebayservicesfilters-plugin.ip.appname}" />
        <property name="endpointUrl" value="https://svcs.ebay.com/ws/spf" />
        <property name="proxyUrl" value="${replyts2-ebayservicesfilters-plugin.proxyurl:}" />
    </bean>

    <bean id="esconfig-user" class="de.mobile.ebay.service.impl.ServiceConfigBean">
        <property name="iafToken" value="${replyts2-ebayservicesfilters-plugin.user.iaftoken}" />
        <property name="appName" value="${replyts2-ebayservicesfilters-plugin.user.appname}" />
        <property name="endpointUrl" value="https://svcs.ebay.com/ws/spf" />
        <property name="proxyUrl" value="${replyts2-ebayservicesfilters-plugin.proxyurl:}" />
    </bean>

    <bean id="esconfig-ipc" class="com.ebay.ecg.gumtree.australia.lbs2.config.TrackingGeolocationConfiguration">
        <constructor-arg name="authorizationUrl" value="${replyts2-ebayservicesfilters-plugin.ip.authUrl}" />
        <constructor-arg name="url" value="${replyts2-ebayservicesfilters-plugin.ip.url}" />
        <constructor-arg name="queryId" value="${replyts2-ebayservicesfilters-plugin.ip.queryId}" />
        <constructor-arg name="proxyHost" value="${replyts2-ebayservicesfilters-plugin.ip.proxyHost}"/>
        <constructor-arg name="proxyPort" value="${replyts2-ebayservicesfilters-plugin.ip.proxyPort}"/>
    </bean>

    <context:component-scan base-package="com.ecg.de.kleinanzeigen.replyts.ebayservicesfilters"></context:component-scan>

</beans>
