{
    "job": {
        "ID": "comaas-TENANT",
        "name": "comaas-TENANT",
        "priority": 50,

        "datacenters": [ "ams1"],
        "region": "comaas-qa",

        "type": "service",

        "TaskGroups": [ {
            "Name":"comaas-node-TENANT",

            "count": COUNT,

            "Tasks": [ {
                "Name":"comaas-TENANT",
                "driver":"raw_exec",
                "Artifacts": [ {
                    "GetterSource":"http://buildmaster001/job/repo-server/ws/GIT_HASH-TIMESTAMP/ARTIFACT",
                    "GetterOptions": {
                        "checksum": "md5"
                    }
                } ],
                "config": {
                    "command":"bin/comaas"
                },
                "LogConfig": {
                    "MaxFileSizeMB":10,
                    "MaxFiles":10
                },
                "Constraints": [ {
                    "LTarget": "${attr.hostname}",
                    "RTarget": "core*",
                    "Operand": "distinct_hosts"
                } ]
            } ]
        } ],

        "Update": {
            "MaxParallel": 1,
            "Stagger": STAGGER
        }
    }
}
